{
  "compilerOptions": {
    "target": "ES2022",
    "lib": ["ES2022"],
    "module": "ES2022",
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "outDir": "../../dist/server",
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "types": ["node"],
    "typeRoots": [
      "../../src/types",
      "../../node_modules/@types"
    ],
    "paths": {
      "xlsx-calc": [
        "../../src/types/xlsx-calc/index.d.ts"
      ]
    }
  },
  "include": [
    "./**/*"
  ],
  "exclude": [
    "../../dist",
    "../../node_modules"
  ]
}